<div class="content-inner">
	<div class="manga-top">
		<div class="list-nav-wrapper">
			<nav class="list-nav chapters">
				{{#each lists}}
				<a class="chapter no-badge" href="#{{this.slug}}"><span class="do-center">{{#if this.byCreator}}<span class="chapter-icon"><svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-message-circle"><path d="M21 11.5a8.38 8.38 0 0 1-.9 3.8 8.5 8.5 0 0 1-7.6 4.7 8.38 8.38 0 0 1-3.8-.9L3 21l1.9-5.7a8.38 8.38 0 0 1-.9-3.8 8.5 8.5 0 0 1 4.7-7.6 8.38 8.38 0 0 1 3.8-.9h.5a8.48 8.48 0 0 1 8 8v.5z"></path></svg></span>{{/if}}{{this.name}}</span></a>
				{{/each}}
			</nav>
		</div>
		<div class="list-core">
			<h1 class="title">Lists</h1>
		<p>Here's your lists. You can modify lists on the manga's details page.</p>

		{{#each lists}}
		{{> list list=this show-home=true }}
		{{/each}}
		</div>
	</div>

</div>

<script src="/js/reusable/lists.js"></script>
<script src="/js/reusable/list-type.js"></script>