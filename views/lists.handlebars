<div class="content-inner">
	<h1 class="title">Lists</h1>
	<p>Here's your lists. You can modify lists on the manga's details page.</p>

	{{#each lists}}
	<section class="section lists">
		<h2 class="sub-title" id="{{slug}}">{{name}}</h2>
		<div class="manga-grid">
			{{#each entries}}
			{{>manga-card manga=this.data}}
			{{/each}}
		</div>
	</section>
	{{/each}}

</div>

<script src="/js/backups.js"></script>